                    if _unit ~= nil\
                            and _unit:isExist()\
                            and _unit:isActive()\
                            and _unit:getLife() > 0\
                             then\
                        if _unit:getCoalition() == 1 then\
                            _foundRed = true\
                        else\
                            _foundBlue = true\
                        end\
                    end\
